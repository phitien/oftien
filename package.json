{
  "name": "oftien",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "oftien": "./bin/index.js"
  },
  "devDependencies": {
    "babel-plugin-wildcard": "^5.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "http-proxy-middleware": "^0.19.1",
    "ip": "^1.1.5",
    "multer": "^1.4.1",
    "node-sass": "^4.11.0",
    "react-scripts": "2.1.3",
    "socket.io": "^2.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "echarts": "^4.2.0-rc.2",
    "echarts-for-react": "^2.0.15-beta.0",
    "gojs": "^2.0.0-beta9",
    "json-stringify-safe": "^5.0.1",
    "jsoneditor": "^5.32.3",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-color": "^2.17.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "setup": "sh setup.sh",
    "build": "echo \"ENV=${ENV:-production}\" > .env && NODE_ENV=production oftien run build",
    "test": "echo \"ENV=${ENV:-local}\" > .env && oftien run test",
    "start": "concurrently \"npm run be\" \"npm run ui\"",
    "ui": "echo \"ENV=${ENV:-local}\" > .env && oftien run start",
    "ui-bg": "echo \"ENV=${ENV:-local}\" > .env && forever start -c \"oftien run start ui\" ./",
    "ui-bg-stop": "forever stop scripts/app.js",
    "be": "echo \"ENV=${ENV:-local}\" > .env && node services/api/app.js",
    "be-bg": "echo \"ENV=${ENV:-local}\" > .env && forever start services/api/app.js",
    "be-bg-stop": "forever stop services/api/app.js",
    "bambu.life": "echo \"ENV=${ENV:-\"bambu.life\"}\" > .env && oftien run start",
    "bambu.life-bg": "echo \"ENV=${ENV:-\"bambu.life\"}\" > .env && forever start -c \"oftien run start ui\" ./"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "ignore": [
      "db/*",
      "test/*"
    ]
  }
}
